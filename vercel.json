{
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "factuvip.com"
        }
      ],
      "destination": "/resellers/factuvip/index.html"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "www.factuvip.com"
        }
      ],
      "destination": "/resellers/factuvip/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
