{
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "(www\\.)?factuvip\\.com"
        }
      ],
      "destination": "/resellers/factuvip/"
    },
    {
      "source": "/((?!resellers).*)",
      "destination": "/index.html"
    }
  ]
}
